# Configuración de Microservicios
PROJECT_SERVICE_URL=http://localhost:8085
PORTAFOLIO_SERVICE_URL=http://localhost:8084

# Configuración de Caché
CACHE_TTL_SECONDS=300

# Configuración de HTTP Client
HTTP_TIMEOUT_SECONDS=30
HTTP_MAX_RETRIES=3
HTTP_RETRY_BACKOFF_FACTOR=0.5

# Configuración de Logging
LOG_LEVEL=INFO

# JWT Token (opcional, para autenticación con microservicios)
# JWT_TOKEN=your_jwt_token_here

# Configuración de Descarga de Imágenes
IMAGE_DOWNLOAD_TIMEOUT=10
IMAGE_DOWNLOAD_MAX_RETRIES=3
IMAGE_BATCH_SIZE=10

# Configuración de Embeddings Visuales
VISUAL_EMBEDDING_CACHE_SIZE_MB=500
